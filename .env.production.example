# Production Environment Configuration
# Copy this file to .env.production and fill in your production credentials

# Firebase Configuration (Production)
EXPO_PUBLIC_FIREBASE_API_KEY=your_prod_firebase_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=millennialsprime.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=millennialsprime
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=millennialsprime.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_prod_messaging_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_prod_app_id
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=your_prod_measurement_id

# API Configuration (Production)
EXPO_PUBLIC_API_BASE_URL=https://us-central1-millennialsprime.cloudfunctions.net/api

# BunnyCDN Configuration (Production)
EXPO_PUBLIC_BUNNYCDN_ACCESS_KEY=your_prod_bunnycdn_access_key
EXPO_PUBLIC_BUNNYCDN_LIBRARY_ID=your_prod_library_id
