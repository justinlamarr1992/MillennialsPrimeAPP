{
  "expo": {
    "name": "Millennials-Prime_APP",
    "slug": "Millennials-Prime_APP",
    "scheme": "your-app-scheme",
    "version": "1.1.6",
    "orientation": "portrait",
    "icon": "./assets/images/mill-prime-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#BD2932"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.justappin.millennials-prime-app",
      "buildNumber": "21",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#BD2932"
      },
      "package": "com.justappin.MillennialsPrime_APP",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "704c67cd-34ad-4f08-b227-a4a10a611879"
      }
    }
  }
}
